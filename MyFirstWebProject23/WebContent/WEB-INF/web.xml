<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>MyFirstWebProject23</display-name>
  <servlet>
  	<servlet-name>ServletForward</servlet-name>
  	<servlet-class>com.junhojohn.controllers.ServletForward</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
  	<servlet-name>ServletRedirect</servlet-name>
  	<servlet-class>com.junhojohn.controllers.ServletRedirect</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ServletForward</servlet-name>
  	<url-pattern>/forwardTest.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletRedirect</servlet-name>
  	<url-pattern>/redirectTest.do</url-pattern>
  </servlet-mapping>
</web-app>